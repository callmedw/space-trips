<%= render "layouts/errors", :object => @review %>

<%= form_for [ @product, @review ] do |f| %>
  <%= f.label :author%>
  <%= f.text_field :author %>
  <%= f.label :content%>
  <%= f.text_area :content %>
  <%= f.label :rating %>
  <%= f.select :rating, ['1', '2', '3', '4', '5'] %>

  <%= f.submit %>
<% end %>
