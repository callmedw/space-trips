<%= render "layouts/errors", :object => @product %>

<%= form_for @product do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :cost %>
  <%= f.text_field :cost %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :image %>
  <%= f.text_field :image %>
  <%= f.label :star %>
  <%= f.text_field :star %>
  <%= f.label :agency %>
  <%= f.text_field :agency %>
  <%= f.label :craft %>
  <%= f.text_field :craft %>
  <%= f.label :distance %>
  <%= f.text_field :distance %>

  <%= f.submit %>
<% end %>
