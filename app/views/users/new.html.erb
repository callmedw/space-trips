<h1> Sign up</h1>
<%= render "layouts/errors", :object => @user %>
<%= form_for @user do |f| %>
  <%= f.label "Email" %>
  <%= f.email_field :email, id: "email" %>
  <%= f.label "Password" %>
  <%= f.password_field :password %>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <%= f.submit "Sign Up" %>
<% end %>
